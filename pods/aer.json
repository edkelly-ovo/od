  "solutions": [
    {
      "name": "3-Pin EV Charger Detection Model",
      "description": "Detects the use of a 3-pin plug for Electric Vehicle (EV) charging based on consumption patterns."
    },
    {
      "name": "Addons",
      "description": "System providing eligibility, signup and lifecycle of add-ons. Also provides integration with Kaluza for crediting and billing for recurring add-ons."
    },
    {
      "name": "Campaign Smart Meter Booking Propensity Model",
      "description": "Estimates the likelihood of a customer booking a smart meter installation in response to a proactive marketing campaign"
    },
    {
      "name": "Change of Supplier Churn Model",
      "description": "A model that specifically predicts the likelihood of a Pay Monthly account switching to another supplier (Change of Supplier) in any given future month."
    },
    {
      "name": "CLV Churn Model",
      "description": "A model that predicts the likelihood of a Pay Monthly account ending its supply in each future month, with predictions extending up to five years, as a component of Customer Lifetime Value (CLV)."
    },
    {
      "name": "CLV Cost To Serve Model",
      "description": "Predicts customer contact duration for the upcoming month, used to allocate total service costs to individual accounts as a component of Customer Lifetime Value (CLV)."
    },
    {
      "name": "CLV Debt Risk Model",
      "description": "A model that predicts the propensity of a customer to default and enter the debt collections process, even if not currently in debt, as a component of Customer Lifetime Value (CLV)."
    },
    {
      "name": "CLV Energy Gross Margin Forecast",
      "description": "Forecasts the future Energy Gross Margin for an account by using predicted consumption, rates, and cost of goods sold, along with modelled tariff behaviour. A core component of Customer Lifetime Value (CLV)."
    },
    {
      "name": "CLV Scenario Tool - DD setup Estimates the change in CLV for a customer after they setup a direct debit",
      "description": "CLV Scenario Tool - DD setup Estimates the change in CLV for a customer after they setup a direct debit"
    },
    {
      "name": "CLV Segmentation",
      "description": "Produces a segmentation of the entire OVO customer base defined by Customer Lifetime Value (CLV), evolving towards a 'value ladder' model"
    },
    {
      "name": "Corgi Retention Model",
      "description": "Predicts the likelihood of a customer renewing their Corgi contract."
    },
    {
      "name": "Credit Check Service",
      "description": "Credit Check Service proxy to Experian"
    },
    {
      "name": "Digital segmentation model",
      "description": ""
    },
    {
      "name": "Direct Debit Setup Propensity Model",
      "description": "Calculates the propensity for a customer to set up a Direct Debit, particularly when prompted by a Next Best Action."
    },
    {
      "name": "Drive Segmentation",
      "description": "Segments customers based on their driving and EV charging behaviours."
    },
    {
      "name": "Energy Address Service",
      "description": "Energy Address Service address service only used for energy switching"
    },
    {
      "name": "Energy Tracker Visit Propensity Model",
      "description": "Calculates the propensity for a customer to visit the Energy Tracker feature."
    },
    {
      "name": "EV Detection Model",
      "description": "Uses energy consumption data to estimate the probability that a customer owns an Electric Vehicle (EV)."
    },
    {
      "name": "EV Disaggregation Model",
      "description": "Estimates the portion of energy consumption attributable to Electric Vehicle (EV) charging from half-hourly usage data"
    },
    {
      "name": "General Smart Meter Booking Propensity Model",
      "description": "Estimates the general likelihood of a customer booking a smart meter installation, based on a model trained with historical data."
    },
    {
      "name": "Greener Energy Lookalike Model",
      "description": "A lookalike model that identifies potential customers for the Greener Energy add-on, used when propensity training data is insufficient."
    },
    {
      "name": "Greener Energy Propensity Model",
      "description": "Estimates the likelihood of a customer purchasing the Greener Energy add-on, based on a model trained with historical data."
    },
    {
      "name": "Heat Pump Detection Model",
      "description": "Uses energy consumption data to estimate the probability that a customer has a heat pump."
    },
    {
      "name": "Heat Pump Simulator",
      "description": "A dual-model system that estimates home heat loss parameters and simulates heat pump consumption based on temperature and property information."
    },
    {
      "name": "Home Moves",
      "description": "Supports customers through the process of moving house, facilitating the transfer of their services to a new address to ensure customer retention."
    },
    {
      "name": "Homeplan Acquisition Lookalike Model",
      "description": "A lookalike model that identifies potential customers for Homeplan products, used when propensity training data is insufficient."
    },
    {
      "name": "Homeplan Acquisition Propensity Model",
      "description": "Estimates the likelihood of a customer purchasing a Homeplan product, based on a model trained with historical data"
    },
    {
      "name": "Homeplan Retention Model",
      "description": "Predicts the likelihood of a customer renewing their Homeplan contract."
    },
    {
      "name": "Onboarding",
      "description": "Orchestrates the end-to-end process of welcoming a new customer to an OVO product, ensuring a smooth and positive initial experience."
    },
    {
      "name": "One Degree Reduction Model",
      "description": "A model to analyse and encourage a one-degree reduction in thermostat settings to save energy."
    },
    {
      "name": "Online Account Digital Action Propensity",
      "description": "Predicts the likelihood of a customer taking a specific action through the OVO web portal or mobile app."
    },
    {
      "name": "Price comparison website partnerships",
      "description": "Price comparison website partnerships"
    },
    {
      "name": "Products",
      "description": "Maintains a definitive catalog of all products and services offered by OVO, serving as the source of truth for product information."
    },
    {
      "name": "Property Partnerships (Tili, Pravda & Pip)",
      "description": "Manages the lifecycle of partner relationships, from registration and account setup to the creation and management of invoices."
    },
    {
      "name": "Quote",
      "description": "Provides customers with personalized, point-in-time pricing for OVO's range of products and services."
    },
    {
      "name": "Renewal",
      "description": "Facilitates the product renewal process for existing customers, aiming to improve customer retention."
    },
    {
      "name": "Renewal and Refix Propensity Model",
      "description": "Predicts the likelihood of a customer renewing their contract or refixing onto a new fixed-term tariff."
    },
    {
      "name": "Smart Thermostat Propensity Model",
      "description": "Estimates the likelihood of a customer purchasing a smart thermostat, based on a model trained with historical data."
    },
    {
      "name": "Solar Installation Detection Model",
      "description": "Uses energy consumption data to estimate the probability that a customer has a solar panel installation."
    },
    {
      "name": "Solar Purchase Propensity Model",
      "description": "Estimates the propensity for a customer to purchase solar panels in response to an email campaign, based on demographic and account features."
    },
    {
      "name": "Switch",
      "description": "Facilitates the customer journey of switching to an OVO product, handling industry-standard processes like Change of Tenancy (COT) and Change of Supplier (COS)."
    },
    {
      "name": "Tariffs",
      "description": "Manages the tariff structures and rates associated with OVO's products, defining how customer usage is priced."
    },
    {
      "name": "Website (logged out)",
      "description": "The primary digital channel for customer acquisition, engagement, and information via ovoenergy.com."
    }
