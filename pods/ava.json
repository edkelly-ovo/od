    },
    {
      "name": "Advisor Knowledge Summary",
      "description": "Advisor driven knowledge base tool to reduce advisor mis-information"
    },
    {
      "name": "AI Advisor Interface",
      "description": "Provides support agents with a dedicated user interface to interact with AI-driven tools and insights, enhancing their ability to assist customers effectively."
    },
    {
      "name": "AI Help",
      "description": "Answers customer chat questions and supports self-serve activities using agentic AI and RAG sourced from help documentation"
    },
    {
      "name": "AVA Customer context POC Service",
      "description": "AVA Customer context POC Service to proactively use customer signals and feedback to influence positive customer behaviours (Next best action, dashboards, self-serve agentic interface)"
    },
    {
      "name": "AVA DPA Automation",
      "description": "Responds to customer emails when they have failed Data Protection Act (DPA) verification."
    },
    {
      "name": "AVA Support Assistant",
      "description": "Acts as a 0-line support agent sitting infront of human agents, using agentic AI, RAG and customer context to handle generic chat support for customers"
    },
    {
      "name": "Balance Optimisation Agent",
      "description": "Answers customer chat questions and supports self-serve activities relating to billing and payments using agentic AI, RAG and customer context"
    },
    {
      "name": "Call experience score",
      "description": ""
    },
    {
      "name": "Call Memos",
      "description": "Uses generative AI to automatically create a memo summary for advisors after each inbound call."
    },
    {
      "name": "Chat Memos",
      "description": "Uses generative AI to automatically create a memo summary for advisors after each inbound chat."
    },
    {
      "name": "Complaint Notes",
      "description": "Summarises the reason and key information for a complaint raised during a telephony interaction to support the agent"
    },
    {
      "name": "Contact complexity classification",
      "description": "Rates a conversation as simple or complex depending on the required actions to resolve the customer problem."
    },
    {
      "name": "Contact topic classification",
      "description": "Provides primary and secondary topics for customer conversations based on transcript."
    },
    {
      "name": "CSAT inference",
      "description": "Predicts the customer satisfaction score a customer would give following a contact."
    },
    {
      "name": "Email DPA and Bereavement Check",
      "description": "Scans inbound emails to check for Data Protection Act (DPA) and bereavement related content."
    },
    {
      "name": "Follow-up Emails",
      "description": "Generates recommended email drafts for customer advisors to review, edit, and send."
    },
    {
      "name": "Metering Overview",
      "description": "Provides advisors with support when booking metering visits in order to maximise likelihood of successful appointments."
    },
    {
      "name": "Mode Switch Action Plan",
      "description": "Real-time plan generation for advisors, to reduce error rate and handling time"
    },
    {
      "name": "NLU Call Routing",
      "description": "Intelligent routing based of voice intent recognition, to improve customer experience and reduce handling time"
    },
    {
      "name": "Propensity to digitally activate",
      "description": "Predicts how likely a given customer is to digitally activate."
    },
    {
      "name": "Realtime NBA",
      "description": "Parsing customer calls to watch for next-best-action plugin points, to increase NBA offer & uptake rate"
    },
    {
      "name": "Vulnerability Detection",
      "description": "Provides real-time prompts to advisors during a call when potential customer vulnerability is detected."
    }
