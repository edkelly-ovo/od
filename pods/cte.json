  "solutions": [
    {
      "name": "Account",
      "description": "Represents the central entity to attribute billing and financial balances, to which one or more customers can be associated."
    },
    {
      "name": "Address Lookup",
      "description": "Provides a service for searching and verifying addresses using postcodes or other identifying information."
    },
    {
      "name": "API Template",
      "description": "A Node.js template repository containing the standard frameworks and tools that OVO uses to build backend APIs."
    },
    {
      "name": "Balance",
      "description": "Calculates and maintains the overall financial balance for a customer account, which can be in credit or debit."
    },
    {
      "name": "Bill Generation",
      "description": "Generates unified bills for all of a customer's OVO products and services, and ensures that final bills are issued within the required SLAs."
    },
    {
      "name": "Bill History",
      "description": "Provides customers and support agents with access to historical billing information. It allows for viewing and downloading past bills, seeing bill breakdowns, requesting paper copies, and managing bill challenges and corrections"
    },
    {
      "name": "Bill Schedule",
      "description": "Allows customers to manage their billing schedule, including changing the frequency of when they receive their bills."
    },
    {
      "name": "Bill Validation",
      "description": "Performs validation checks on both regular and final bills to ensure their accuracy before they are issued to the customer."
    },
    {
      "name": "Billing",
      "description": "Manages the core billing process, including creating bills from various charges, handling estimated reads, managing unbilled accounts, processing final bills for customers leaving, and placing or removing holds on bill calculations."
    },
    {
      "name": "Customer",
      "description": "Represents an individual who has a relationship with OVO, holding personal details and linking to many other entities."
    },
    {
      "name": "Customer Timeline",
      "description": "Maintains a chronological record of all significant events and interactions related to a customer's journey with OVO."
    },
    {
      "name": "Eligibility",
      "description": "Determines if a customer, account, or property meets the necessary criteria to qualify for specific OVO products or promotional offers."
    },
    {
      "name": "Energy Account",
      "description": "Represents the entity to attribute energy usage, energy billing and associated electricity- and gas-specific functionality"
    },
    {
      "name": "Grid Carbon Intensity",
      "description": "Provides real-time data on the carbon intensity of the national grid, empowering customers to make more sustainable energy choices."
    },
    {
      "name": "Kaluza",
      "description": "Kaluza"
    },
    {
      "name": "KAPI Emulator",
      "description": "Testing tooling to emulate mocking interactions with Kaluza over either Kapi or Kafka"
    },
    {
      "name": "NodeJS Library Template",
      "description": "A Node.js template repository containing the standard frameworks and tools that OVO uses to build libraries."
    },
    {
      "name": "Order",
      "description": "Manages the end-to-end lifecycle of a customer's request for one or more OVO products or services."
    },
    {
      "name": "Property",
      "description": "Represents a physical address or location where OVO provides services, linked to supply points and utility assets."
    },
    {
      "name": "Spectral Ruleset",
      "description": "A set of linting rules that are used to help big consistency between all APIs that OVO builds."
    },
    {
      "name": "Supply Point Asset",
      "description": "Represents the physical entity at a specific property identified by a unique industry identifier, such as an MPAN or MPRN, for a gas or electricity supply"
    },
    {
      "name": "Supply Point Lookup",
      "description": "Enables the identification and retrieval of specific energy supply points (MPAN/MPRN) associated with a given address."
    },
    {
      "name": "Tap",
      "description": "A data connector that streams data from Kaluza's KEG strategic event gateway to OVO's internal Kafka and Pub/Sub clusters"
    },
    {
      "name": "Tech Handbook",
      "description": "A comprehensive, high-level guide to OVO's technology landscape, culture, and best practices. Provides a single source of truth for technology principles, fosters a shared understanding of how we build software, and is used to onboard new engineers."
    },
    {
      "name": "Utility Asset",
      "description": "Represents and manages the physical meters that measure energy supply at a customer's property."
    },
    {
      "name": "Utility Asset Eligibility",
      "description": "Determines a property's eligibility for specific utility assets, such as smart meter upgrades (e.g., SMETS2)."
    },
    {
      "name": "Utility Contract",
      "description": "Manages the terms of a customer's energy contract, including tariff terms and rates"
    },
    {
      "name": "Utility Fulfilment",
      "description": "Orchestrates the complex processes involved in establishing and maintaining the supply of electricity and gas to a customer's property."
    },
    {
      "name": "Utility Offboarding",
      "description": "Orchestrates the process of ending a customer's energy supply, including final billing and industry flow management."
    },
    {
      "name": "Utility Usage",
      "description": "Tracks and records a customer's energy consumption data (e.g., in kWh), which serves as the basis for billing."
    },
    {
      "name": "Utility Usage Efficiency",
      "description": "Analyses a customer's energy consumption and provides insights into their usage efficiency compared to similar households."
    },
    {
      "name": "Utility Usage Readings",
      "description": "Captures and processes utility meter readings from various sources, including automated smart meters, manual entries, and customer submissions."
    }
  ],
