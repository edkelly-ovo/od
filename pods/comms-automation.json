  "solutions": [
    {
      "name": "Comms Decisioning",
      "description": "Operationalisation of Customer Comms Strategies, Contact Rules and Commercial priorities to provide optimised customer contact levels and outcomes"
    },
    {
      "name": "Comms execution platform",
      "description": "Multi-stage, multi- channel and multi-product comms built and managed on SFMC"
    },
    {
      "name": "Comms Snapshot Process",
      "description": "Process to generate an image of the comms sent to a customer"
    },
    {
      "name": "Customer Audience build",
      "description": "Latest customer data, insight and consents used to build target audiences for comms"
    },
    {
      "name": "Document Repository",
      "description": "Tool with UI that allows non-SF users to see comms that have been sent to customer by customer, by comm"
    },
    {
      "name": "Feedback API",
      "description": "End point that allows OVO teams to obtain contact history and visualisation of content sent to customers."
    },
    {
      "name": "Print Fulfilment",
      "description": "Development and maintenance of print fulfilment solution for Comms Automation and other OVO teams"
    },
    {
      "name": "Self Serve of Comms",
      "description": "Self serve of comms origination and delployment for comms teams to allow relevant comms to be executed more quickly and with increased capacity for continuous improvement"
    },
    {
      "name": "Service Comms Measurement",
      "description": "For all Service Comms now on the Comms Automation Service, monitoring and presentment of Operational and Performance data to allow fast issue identification, fixes and continuous improvement"
    },
    {
      "name": "Single Marketing View",
      "description": "Multi-product view of customer details, product holding, content and other insight required for the fulfilment of marketing comms"
    }
  ],
