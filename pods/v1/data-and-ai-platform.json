  "solutions": [
    {
      "name": "AI/ML Platform",
      "description": "A scalable platform for building and deploying machine learning models, providing the infrastructure for AI-driven services, including Model Armor"
    },
    {
      "name": "AutoCapture",
      "description": "A data connector that streams data from Kafka into BigQuery, intended to replace a previous version to improve security, reliability, and enable Kaluza separation."
    },
    {
      "name": "BigQuery Access auditing",
      "description": "Tool to list users who have access to BigQuery data and their query history."
    },
    {
      "name": "Data Access",
      "description": "Data Catalogue and Data Access requests. To be replaced by DataHub in time."
    },
    {
      "name": "Data Classification",
      "description": "The Darwin tool scans BigQuery data for personal data and makes classification recommendations."
    },
    {
      "name": "Data Observability",
      "description": "Data Observability and Quality tool (Monte Carlo)"
    },
    {
      "name": "Data Platform",
      "description": "A new centralised platform for data."
    },
    {
      "name": "Data Warehouse",
      "description": "Core Energy data in a consolidated SQL-consumable data model"
    },
    {
      "name": "DataHub (Data Catalogue)",
      "description": "A central data catalogue that includes a business glossary, data lineage, and product documentation, which will provide the primary route to access data in the future"
    },
    {
      "name": "DataRobot",
      "description": "A comprehensive platform for the deployment, monitoring, and management of data science models across the organisation."
    },
    {
      "name": "GenAI/LLM template",
      "description": "A simple template for users to deploy a Python application to Evergreen to interact with Gemini"
    },
    {
      "name": "Langsmith",
      "description": "GenAI observability, and evaluation platform"
    },
    {
      "name": "Looker",
      "description": "Data visualisation tool"
    },
    {
      "name": "ML Template",
      "description": "A template for users to experiment, and deploy machine learning models"
    },
    {
      "name": "Source Data Mirror",
      "description": "Pipelines streaming raw data from a set of Apollo (including Gentrack, Ballard Chalmers) source platforms' SQL databases, to Kafka and BigQuery. To be handed over to domain teams in time."
    },
    {
      "name": "Tablaeu",
      "description": "Data visualisation tool"
    },
    {
      "name": "Thoughtspot",
      "description": "Data visualisation tool"
    }
  ],
