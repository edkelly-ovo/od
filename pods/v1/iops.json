  "solutions": [
    {
      "name": "Agent",
      "description": "Tooling (UI) React backoffice agent application to support agents to identify and resolve exceptions - primarily settlement fixes"
    },
    {
      "name": "Camunda",
      "description": "Process and microservice orchestration platform"
    },
    {
      "name": "Centrestage",
      "description": "Read only data warehouse solution storing detailed information on ESG and the DCC data flows too and from Smarter Meters, includes meter readings, tarrif information, changes to meters, MTDS etc... Designed for analysis use NOT operational use cases or automations"
    },
    {
      "name": "Consent",
      "description": "Manages and records customer consents and preferences for data sharing, ensuring compliance with regulatory requirements. Data is supplied from Kaluza"
    },
    {
      "name": "Exceptions",
      "description": "Manages and tracks exceptions from automated industry processes, creating tasks for support agents to ensure efficient manual intervention and resolution"
    },
    {
      "name": "Inbound Industry Interactions Processor",
      "description": "Processes incoming data flows from external industry bodies, ensuring data is validated, transformed, and routed to the correct internal systems."
    },
    {
      "name": "Industry Data Service",
      "description": "Ingests industry flows and reports, facade over ESG Data Streams (i.e for data not in KEG / KAPI) Enables us to identify discrepancies in industry data, notification events are created from ingested information, provides a REST API (JSON) view of flows (Aim to replace Kaluza Kafka integrations for industry flows)"
    },
    {
      "name": "Keycloak",
      "description": ""
    },
    {
      "name": "Outbound Industry Interactions Generator",
      "description": "Generates and transmits outgoing data flows to external industry bodies, ensuring compliance with industry communication standards"
    },
    {
      "name": "Reads API",
      "description": "Consumes data from the Industry data service presents as unified UtilityReading source for Elec and Gas meters"
    },
    {
      "name": "Themis",
      "description": ""
    },
    {
      "name": "Utility / Cross Cutting Platform and Services",
      "description": ""
    },
    {
      "name": "Utility Usage Settlement",
      "description": "Manages the settlement of energy costs with generators and the grid, based on aggregated customer usage data to ensure accurate financial reconciliation"
    },
    {
      "name": "UtilityAsset API",
      "description": "Provides a source of truth for meter location data - aim is to allow mutation of meter data encapsulating industry interactions / orchestration to do so"
    }
  ],
