  "solutions": [
    {
      "name": "enTrader",
      "description": "Energy Trading and Risk Management (ETRM) solution - enables multiple teams in EMO to make decisions and data from enTrader feeds into lots of reporting"
    },
    {
      "name": "Flex Platform",
      "description": "Enables ovo to bid for market events"
    },
    {
      "name": "Greener Grid platform",
      "description": "Quotes, pricing and invoicing for PPA customers (i.e. large wind or solar farms)"
    },
    {
      "name": "Mercury",
      "description": "Enables short-term gas and power traders to see their position and execute auction trades"
    },
    {
      "name": "Position",
      "description": "Provides real-time visibility into OVO's market exposure, enabling improved cash flow management, risk mitigation and rapid response to market events."
    },
    {
      "name": "Secondary Market",
      "description": "Manages demand-side flexibility schemes, enabling customers to participate and receive compensation for adjusting their energy usage via their flexible assets."
    },
    {
      "name": "Secondary Market Asset",
      "description": "Manages the lifecycle of customer assets participating in secondary markets, including eligibility, reservations and interaction with bidding engines."
    },
    {
      "name": "Secondary Market Competition",
      "description": "Interfaces with external market competitions, automatically submitting bids based on the aggregated availability of customer-owned flexible assets."
    },
    {
      "name": "Trade",
      "description": "Manages the procurement of energy commodities from third-party suppliers to meet OVO's gas and electricity obligations."
    },
    {
      "name": "Trade Auction",
      "description": "Facilitates energy procurement auctions, allowing OVO to nominate and engage with commodity counterparties to secure trades."
    },
    {
      "name": "Trade Prediction",
      "description": "Leverages data analytics to forecast market trends and predict optimal trading positions over various time horizons."
    },
    {
      "name": "Weather service",
      "description": ""
    }
  ],
